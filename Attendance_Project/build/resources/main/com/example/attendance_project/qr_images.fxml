   <children>
      <ImageView fx:id="qr_image" fitHeight="233.0" fitWidth="311.0" layoutX="185.0" layoutY="116.0" pickOnBounds="true" preserveRatio="true" />
      <Label layoutX="188.0" layoutY="14.0" prefHeight="45.0" prefWidth="305.0" text="Scan the QR">
         <font>
            <Font name="System Bold" size="51.0" />
         </font>
      </Label>
      <Button fx:id="back" layoutX="24.0" layoutY="32.0" mnemonicParsing="false" onAction="#backButtonAction" prefHeight="38.0" prefWidth="95.0" text="Back" />
   </children>
</AnchorPane>
